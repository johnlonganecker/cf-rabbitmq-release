---
message_bus_servers:
  <% p("route_registrar.message_bus_servers").each do |server| %>
  - host: <%= server["host"] %>:<%= server["port"] %>
    user: <%= server["user"] %>
    password: <%= server["password"] %>
  <% end %>
host: <%= spec.address %>
routes:
  - name: <%= p('rabbitmq-broker.rabbitmq.management_domain') %>
    registration_interval: <%= p('rabbitmq-broker.registration_interval')%>
    port: 15672
    uris:
      - <%= p('rabbitmq-broker.rabbitmq.management_domain') %>
